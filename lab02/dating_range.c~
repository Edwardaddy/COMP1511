#include<stdio.h>

int main(void){
    int a1;
    int a2;
    int a3;

    printf("Enter your age:");
    scanf("%d",&a1);
    
    a2=a1/2+7;
    a3=(a1+7)*2;
    
    if(a1<12){
    printf("You are too young to be dating.\n");
    }else{
    printf("Your dating range is %d to %d years old.\n",a2,a3);
    }
    return 0;
}
